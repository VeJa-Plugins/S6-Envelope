@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://VeJaPlugins.com/plugins/Release/s6Envelope>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-s6-envelope.html> ;
        modgui:stylesheet <modgui/stylesheet-s6-envelope.css> ;
        modgui:screenshot <modgui/screenshot-s6-envelope.png> ;
        modgui:thumbnail <modgui/thumbnail-s6-envelope.png> ;
        modgui:brand "Veja" ;
        modgui:label "S6 Envelope" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "black" ;
        modgui:knob "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "ENV_A" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "ENV_D" ;
            lv2:name "Decay" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "ENV_S" ;
            lv2:name "Sustain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "ENV_R" ;
            lv2:name "Release" ;
        ] ;
    ] .